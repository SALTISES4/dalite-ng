  },
  "prettier": {
    "printWidth": 79,
    "parser": "flow",
    "trailingComma": "all"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "preact",
      "google",
      "prettier"
    ],
    "globals": {
      "$": "readonly",
      "bundle": "readonly",
      "gettext": "readonly"
    },
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "strict": 0,
      "no-invalid-this": "off",
      "arrow-parens": "off",
      "indent": [
        "error",
        2
      ],
      "require-jsdoc": "off",
      "guard-for-in": "off",
      "quotes": [
        "error",
        "double"
      ],
      "radix": "off",
      "react/prefer-stateless-function": "off",
      "spaced-comment": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prettier/prettier": "error"
    }
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss",
      "stylelint-order"
    ],
    "extends": "stylelint-config-recommended-scss",
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "no-empty-source": null,
      "order/properties-alphabetical-order": true,
      "no-descending-specificity": null,
      "block-closing-brace-empty-line-before": "never"
    }
  },
  "browserslist": [
    "last 3 versions and > 0.1%",
    "not IE 11",
    "not IE_Mob 11",
    "not dead"
  ]
}
